{
  "name": "age-plotter-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run build:monaco",
    "build:js": "node scripts/esbuild.mjs",
    "build:css": "tailwindcss -i src/age_plotter/static/css/input.css -o src/age_plotter/static/css/styles.css --minify",
    "build:monaco": "node scripts/copy-monaco.mjs",
    "watch:js": "node scripts/esbuild.mjs --watch",
    "watch:css": "tailwindcss -i src/age_plotter/static/css/input.css -o src/age_plotter/static/css/styles.css --watch"
  },
  "dependencies": {
    "cytoscape": "^3.33.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-d3-force": "^1.1.3",
    "cytoscape-fcose": "^2.2.0",
    "d3": "^7.9.0",
    "htmx.org": "^2.0.3",
    "monaco-editor": "^0.45.0"
  },
  "devDependencies": {
    "daisyui": "^4.12.14",
    "esbuild": "^0.20.0",
    "tailwindcss": "^3.4.0"
  }
}
